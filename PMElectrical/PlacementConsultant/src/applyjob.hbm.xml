<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="com.virtusa.placement.pojo.ApplyJob" table="APPLYJOB">
	   <id name="sno"  column="SNO_APPLIED" >
	   <generator class="sequence">
	   <param name="sequence">sno_applied_seq</param>
	   </generator>	 
	   </id>
	   <property name="userName">
	   <column name="USERNAME"></column>
	   </property>
	   <property name="jobId">
	   <column name="JOB_ID"></column>
	   </property>	  
	   <property name="appliedDate">
	   <column name="APPLIED_DATE"></column>
	   </property>
  </class>
   
</hibernate-mapping>