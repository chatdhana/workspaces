<?xml version="1.0" encoding="UTF-8"?>
<ejb-jar version="3.0" xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/ejb-jar_3_0.xsd">

	<enterprise-beans>
		<session>
			<display-name>Hello</display-name>
			<ejb-name>Hello</ejb-name>
			<home>myejb.sb.stateless.HomeInterface</home>
			<remote>myejb.sb.stateless.RemoteInterface</remote>
			<local-home>
				myejb.sb.stateless.LocalHomeInterface
			</local-home>
			<local>myejb.sb.stateless.LocalInterface</local>
			<ejb-class>myejb.sb.stateless.HelloBean</ejb-class>
			<session-type>Stateless</session-type>
			<transaction-type>Container</transaction-type>
		</session>
	</enterprise-beans>
</ejb-jar>